(executable
  (name SketchJsoo)
  (public_name sketch.jsoo)
  (package sketch)
  (flags :standard -safe-string)
  (js_of_ocaml 
   (flags :standard --no-cmis --toplevel --linkall +toplevel.js +dynlink.js)
  )
  (modes byte)
  (preprocess (pps js_of_ocaml-ppx))
  (link_flags -linkall)
  (libraries 
    js_of_ocaml 
    js_of_ocaml-toplevel
    console.lib
    sketch.util
    sketch.repl
    sketch.core
    sketch.read_stdout_js
  )
)
